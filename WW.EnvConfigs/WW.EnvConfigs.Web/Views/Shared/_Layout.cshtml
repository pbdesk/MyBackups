                <script src="~/NGApp/scripts/services/KeyValueFactory.js"></script>
                <script src="~/NGApp/scripts/services/LocalesFactory.js"></script>
                <script src="~/NGApp/scripts/services/EnvFactory.js"></script>
                <script src="~/NGApp/scripts/services/AppsAndFwksFactory.js"></script>

                <script src="~/NGApp/scripts/controllers/KeysControllers.js"></script>
                <script src="~/NGApp/scripts/controllers/LocalesControllers.js"></script>
                <script src="~/NGApp/scripts/controllers/ValueControllers.js"></script>
        <script src="~/NGApp/scripts/controllers/OkCancelDialogInstanceController.js"></script>

